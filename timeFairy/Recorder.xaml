<Window x:Class="timeFairy.Recorder"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:timeFairy"
        mc:Ignorable="d"
        Title="Recorder" Height="450" Width="800">
    <Grid>
        
        <TextBox Name="textBox" HorizontalAlignment="Left" Height="43.851" Margin="192.288,187.038,0,0" TextWrapping="Wrap" Text="00:00:00" VerticalAlignment="Top" Width="137.819"/>
        <Button Content="开始" Click="Start_Click" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="69.954,146.171,0,0"/>
        <Button Content="暂停" Click="Pause_Click" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="238.051,146.171,0,0"/>
        <Button Content="清零" Click="End_Click" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="380.047,146.171,0,0"/>
        <Button Content="完成" Click="Close_Click" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="509.512,146.171,0,0"/>
        <Button Content="Button" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75"/>
        <Label Name="label" Content="00:00:00" HorizontalAlignment="Left" Height="40.719" Margin="125.466,262.211,0,0" VerticalAlignment="Top" Width="146.172"/>

    </Grid>
</Window>
